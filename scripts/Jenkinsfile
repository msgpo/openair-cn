pipeline {
    // Currently using abeille with Raphael's credentials
    // We need to investigate how to use Orion concurrently with development efforts
    agent {
        label 'abeille'
    }
    options {
        disableConcurrentBuilds()
        timestamps()
    }
    environment {
        dns1 = "192.168.106.10"
        dns2 = "192.168.106.12"
        vm_display = "False"
    }

    stages {
        stage ('Verify Parameters') {
            steps {
                echo 'Verify Parameters'
                echo "Base URL   is ${JENKINS_URL}"
                echo "Git Branch is ${GIT_BRANCH}"
                echo "Git Commit is ${GIT_COMMIT}"
                echo "Git URL    is ${GIT_URL}"
                sh 'printenv | grep dns'
                //echo "DNS1 Addr  is ${dn1}"
                //echo "DNS2 Addr  is ${dn2}"
                //echo "VM Display is ${vm_display}"
            }
        }
    }
}
